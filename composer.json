{
	"description": "Retrieves Photos Based On A Hashtag",
	"name": "Hashtag Get My Photos",
	"type": "library",
	"autoload": {
		"psr-4": {
			"HashTagGetMyPhotos\\Factory\\" : "src/factories/",
			"HashTagGetMyPhotos\\Products\\" : "src/products/",
			"HashTagGetMyPhotos\\Config\\" : "src/config/"
		}
	},
	"repositories": [{
		"type": "git",
		"url": "git@github.com:LoganConnor44/coding-standard-enforcement.git"
	}],
	"require-dev": {
		"phpunit/phpunit": "^6.1",
		"squizlabs/php_codesniffer": "3.*",
		"loganconnor44/coding-standard-enforcement": "dev-master"
	},
	"require": {
		"guzzlehttp/guzzle": "~6.0",
		"j7mbo/twitter-api-php": "^1.0"
	},
	"scripts": {
		"post-install-cmd": [
			"bash contrib/setup-hook.sh"
		]
	}
}
